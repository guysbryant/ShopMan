<%= render partial: "layouts/errors", locals: {object: @product} %>
<%= form_for @product do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <p>

    <%= f.label :description %>
    <%= f.text_field :description %>
    <p>
    <%= f.label :part_number %>
    <%= f.text_field :part_number %>
    <p>

    <%= f.submit %>
<% end  %>
