<%= render partial: "login_error" %>
<h1>Login</h1>
<%= form_tag "/login" do %>
    <p>
        <%= label_tag "Email" %>
        <%= text_field_tag :email %>
    </p>
    <p>
        <%= label_tag "Password" %>
        <%= password_field_tag :password %>
    </p>
    <%= submit_tag "Login" %>
<% end %>
