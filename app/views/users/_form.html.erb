<%= render partial: "layouts/errors", locals: {object: @user} %>
<%= form_for @user do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :email %>
        <%= f.text_field :email %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>
    <p>
        <%= f.label :address %>
        <%= f.text_field :address %>
    </p>
    <p>
        <%= f.label :phone_number %>
        <%= f.telephone_field :phone_number %>
    </p>
    <%= f.submit %>
<% end %>
